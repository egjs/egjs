{
    "tags": {
        "allowUnknownTags" : true,
        "dictionaries": ["jsdoc","closure"]
    },
    "plugins": ["plugins/markdown","plugins/ko", "plugins/group", "plugins/codepen", "plugins/support"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles" : true
        },
        "applicationName": "egjs",
        "disqus": "egjs",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "egjs - API",
            "description": "egjs is a jQuery-based JavaScript library consisting of UI interactions, effects, and utilities, which brings easiest and fastest way to build a web application in your way.",
            "keyword": "jQuery, UI Component, egjs"
        },
        "linenums": true,
        "link": {
            "canonical": "http://naver.github.io/egjs/latest/doc/"
        }
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true,
        "tags": ["examples", "ko"]
    }
}
